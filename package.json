    "prettier": "2.0.5",
    "react-inlinesvg": "1.2.0",
    "stylelint": "13.5.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
