    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "file-loader": "6.0.0",
